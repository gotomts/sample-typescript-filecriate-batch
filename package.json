{
  "name": "typescript-s3-fileupload",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "exec": "./node_modules/.bin/env-cmd node ./dist/main.js",
    "test": "jest --runInBand",
    "test:summary": "jest --runInBand --coverage --coverageDirectory=output/coverage --reporters=jest-junit",
    "build": "npm-run-all clean tsc",
    "dev": "./node_modules/.bin/env-cmd ts-node ./src/main.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "clean": "rimraf dist",
    "jest": "jest --runInBand --verbose --coverage",
    "tsc": "tsc --build tsconfig.build.json",
    "commit": "git-cz",
    "typedoc": "typedoc --readme ./README.md --out ./doc .src",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create -n",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/.bin/typeorm schema:drop"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gotomts/typescript-s3-fileupload.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gotomts/typescript-s3-fileupload"
  },
  "homepage": "https://github.com/gotomts/typescript-s3-fileupload#readme",
  "devDependencies": {
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/prettier": "^2.1.5",
    "@types/rimraf": "^3.0.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5",
    "typesync": "^0.8.0"
  },
  "dependencies": {
    "aws-sdk": "^2.794.0",
    "env-cmd": "^10.1.0",
    "faker": "^5.1.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.29",
    "typeorm-seeding": "^1.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "collectCoverageFrom": [
      "**/src/**/*.ts"
    ],
    "testMatch": [
      "**/test/**/*.+(ts|js)"
    ],
    "coverageReporters": [
      "test",
      "cobertura",
      "json",
      "lcov",
      "clover"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "output/junit",
    "outputName": "jest-junit.xml"
  }
}